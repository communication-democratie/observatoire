<div id="<%= dom_id report %>">
  <p>
    <%= report.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= report.description %>
  </p>

  <p>
    <strong>Author pseudonym:</strong>
    <%= report.author_pseudonym %>
  </p>

  <p>
    <strong>Author place:</strong>
    <%= report.author_place %>
  </p>

  <p>
    <strong>Author contact:</strong>
    <%= report.author_contact %>
  </p>

</div>
